{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Usage Data Provider Schema",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "label": {
      "type": "string"
    },
    "vendorId": {
      "type": "string"
    },
    "vendorName": {
      "type": "string"
    },
    "platformId": {
      "type": "string"
    },
    "harvestingStatus": {
      "type": "string",
      "enum": [
        "active",
        "inactive",
        "in process",
        "not possible"
      ]
    },
    "aggregator": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "vendorCode": {
          "type": "string"
        }
      },
      "required": [
        "id"
      ]
    },
    "serviceType": {
      "type": "string"
    },
    "serviceUrl": {
      "type": "string"
    },
    "reportRelease": {
      "type": "integer"
    },
    "requestedReports": {
      "type": "array",
      "minItems": 0,
      "items": {
        "type": "string"
      }
    },
    "customerId": {
      "type": "string"
    },
    "requestorId": {
      "type": "string"
    },
    "apiKey": {
      "type": "string"
    },
    "requestorName": {
      "type": "string"
    },
    "requestorMail": {
      "type": "string"
    },
    "createdDate": {
      "type": "string",
      "format": "date-time"
    },
    "updatedDate": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "id",
    "label",
    "vendorId",
    "platformId",
    "harvestingActive",
    "reportRelease",
    "requestedReports",
    "customerId",
    "requestorId"
  ],
  "additionalProperties": false
}