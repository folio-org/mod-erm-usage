{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Aggregator Settings Schema",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "label": {
      "type": "string"
    },
    "username": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "apiKey": {
      "type": "string"
    },
    "serviceType": {
      "type": "string"
    },
    "serviceUrl": {
      "type": "string"
    },
    "accountConfig": {
      "type": "object",
      "properties": {
        "configType": {
          "type": "string",
          "enum": [
            "Mail",
            "API",
            "Manual"
          ]
        },
        "configMail": {
          "type": "string"
        },
        "displayContact": {
          "type": "array",
          "minItems": 0,
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "configType",
        "configMail"
      ]
    },
    "createdDate": {
      "type": "string",
      "format": "date-time"
    },
    "updatedDate": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "id",
    "label",
    "username",
    "password",
    "serviceUrl",
    "accountConfig"
  ],
  "additionalProperties": false
}