{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Sushi Settings Schema",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "requestor_id": {
      "type": "string"
    },
    "customer_id": {
      "type": "string"
    },
    "api_key": {
      "type": "integer"
    },
    "counter_providers": {
      "type": "array",
      "minItems": 0,
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "id": {
            "type": "string"
          }
        }
      }
    }
  },
  "additionalProperties": false
}